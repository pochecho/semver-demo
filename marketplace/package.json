{
  "name": "market-place-components",
  "version": "1.2.25-alfa",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-prod": "ng build --prod --configuration=production",
    "build-qa": "ng build --prod --configuration=qa",
    "build:lib": "ng build ng_bds",
    "build:lib:dev": "cd ../bancolombiaDesignSystem && gulp build_lib_dev",
    "build:lib:qa": "cd ../bancolombiaDesignSystem && gulp build_lib_qa",
    "test": "ng test --watch=false --code-coverage ",
    "test:app": "ng test --watch=false",
    "lint": "ng lint --fix",
    "e2e": "ng e2e",
    "doc:py": "py auto_docs_lib.py",
    "doc:at": "cd projects/ng_bds && npm run compodoc && cd ../../ && npm run doc:py",
    "test:full": "ng test && cd ../bancolombiaDesignSystem/ts && npm run test",
    "gulp:w": "cd ../bancolombiaDesignSystem && start npm run start",
    "gulp": "cd ../bancolombiaDesignSystem && npm run start",
    "ag": "start ng serve -c=local && cd ../bancolombiaDesignSystem && start npm run start",
    "galatea-guidelines": "cd scripts && py guidelines/main.py",
    "test:bds:dev": "ng test ng_bds --code-coverage --watch=true",
    "test:showcase:dev": "ng test show-case --code-coverage --watch=true",
    "test:bds": "ng test ng_bds --code-coverage --watch=false",
    "test:showcase": "ng test show-case --code-coverage --watch=false"
  },
  "private": false,
